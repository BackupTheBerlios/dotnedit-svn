	LTXMode>Edit Sources@TeX-Log:::: {\n\
		# get line number, either selected or under cursor\n\
		if ($selection_start!=-1) {\n\
		    number=get_selection()\n\
		    }\n\
		    else {\n\
		      start=search("[^0-9][0-9]+",$cursor,"regex","backward")\n\
		      if (start==-1) {\n\
		          dialog("Missing line number to go to!")\n\
		          return\n\
		          }\n\
		      end=search("[0-9]*[^0-9]",$cursor,"regex")\n\
		      if (end==-1) {\n\
		          dialog("Missing line number to go to!")\n\
		          return\n\
		          }\n\
		      number=get_range(start+1,$search_end-1)\n\
		      }\n\
		\n\
		   \n\
		#dialog(number)\n\
		\n\
		# get file\n\
		start=search("\\\\(.*\\\\.tex",$cursor,"regex","backward")\n\
		if (start==-1) {\n\
		    dialog("file search failed")\n\
		    return\n\
		    }\n\
		file=get_range(start+1,$search_end)\n\
		if ($main_path!="") {\n\
		    path=$main_path\n\
		    }\n\
		    else {\n\
		      path=$file_path\n\
		      }\n\
		    \n\
		# number test\n\
		if (search_string(number,"[^0-9]",0,"regex")!=-1) {\n\
		    dialog("not a number:\\n" number)\n\
		    return\n\
		    }\n\
		open(path file)\n\
		focus_window(path file)\n\
		goto_line_number(number)\n\
	}\n\
