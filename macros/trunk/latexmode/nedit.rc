		        {\n\
			beep()\n\
			}\n\
	}\n\
	Comments>ues2syntax@LaTeX@SGML HTML:::: {\n\
		ues[1]="ü"\n\
		ues[2]="ö"\n\
		ues[3]="ä"\n\
		ues[4]="ß"\n\
		ues[5]="Ü"\n\
		ues[6]="Ö"\n\
		ues[7]="Ä"\n\
		\n\
		if ($language_mode=="LaTeX" || $language_mode=="Plain")\n\
		{\n\
		tex[1]="\\"u"\n\
		tex[2]="\\"o"\n\
		tex[3]="\\"a"\n\
		tex[4]="\\"s"\n\
		tex[5]="\\"U"\n\
		tex[6]="\\"O"\n\
		tex[7]="\\"A"\n\
		}\n\
		else if ($language_mode=="SGML HTML")\n\
		{\n\
		tex[1]="&uuml;"\n\
		tex[2]="&ouml;"\n\
		tex[3]="&auml;"\n\
		tex[4]="&szlig;"\n\
		tex[5]="&Uuml;"\n\
		tex[6]="&Ouml;"\n\
		tex[7]="&Auml;"\n\
		}\n\
		else\n\
		{\n\
		dialog("Only for Plain, LaTeX or HTML","OK")\n\
		return\n\
		}\n\
		but=dialog("Your choice","ü -> " tex[1],tex[1] " -> ü","Cancel")\n\
		if (but==3) return\n\
		\n\
		if ($selection_start==-1)\n\
			{\n\
			if (but == 1)\n\
				{\n\
				for (aKey in ues) replace_all(ues[aKey],tex[aKey],"case")\n\
				}\n\
			if (but == 2) \n\
				{\n\
				for (aKey in ues) replace_all(tex[aKey],ues[aKey],"case")\n\
				}\n\
			}\n\
			else\n\
			   {\n\
			   if (but == 1)\n\
			   	{\n\
				for (aKey in ues) replace_in_selection(ues[aKey],tex[aKey],"case")\n\
				}\n\
			   if (but == 2) \n\
			   	{\n\
				for (aKey in ues) replace_in_selection(tex[aKey],ues[aKey],"case")\n\
				}\n\
			   }	 \n\
		\n\
	}\n\
	Comments>doc_strip@NEdit Macro@LaTeX:::: {\n\
		com_pat["LaTeX"]="(?n%.*?$)"\n\
		com_pat["NEdit Macro"]="(?n#.*?$)"\n\
		\n\
		if ($language_mode in com_pat)\n\
		   {\n\
		   if ($selection_start == -1) \n\
		      {\n\
		      text = get_range(0,$text_length)\n\
		      text = doc_strip(text,com_pat[ $language_mode])\n\
		      select_all()\n\
		      replace_selection(text)\n\
		      }\n\
		      else \n\
		      {\n\
		      text = get_selection()\n\
		      text = doc_strip(text,com_pat[ $language_mode])\n\
		      replace_selection(text)\n\
		      }\n\
		   }\n\
		   else\n\
		      {\n\
		      return\n\
		      }  \n\
	}\n
