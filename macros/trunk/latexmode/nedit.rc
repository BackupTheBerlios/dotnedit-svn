	NMacro>Load functions@NEdit Macro:Alt+0::: {\n\
		cur_file = $file_path $file_name\n\
		t = read_file(cur_file)\n\
		if ($read_status != 0) {\n\
		    # file exists on disk, ie it is not untitled window\n\
		    if ($modified == 1) save()\n\
		    load_macro_file(cur_file)\n\
		    }\n\
	}\n\
	NMacro>Run macro:Alt+9::: {\n\
		start_window = $file_path $file_name\n\
		list = ""\n\
		filename = focus_window("last")\n\
		while(filename != "") {\n\
		    if ($language_mode == "NEdit Macro") {\n\
		        if (list != "") {\n\
		            list = filename "\\n" list\n\
		            }\n\
		            else {\n\
		            list = filename\n\
		            }\n\
		        }\n\
		    filename = focus_window("next")\n\
		    }\n\
		\n\
		macro_window = list_dialog("Select macro window : ", list, "OK", "Cancel")\n\
		\n\
		if (macro_window == "" || $list_dialog_button == 2) {\n\
		    beep()\n\
		    return\n\
		    }\n\
		# focus to the window where the macro is located\n\
		focus_window(macro_window)\n\
		if ($selection_start != -1) {\n\
		    macro = get_selection() "\\n"  # macro must end with newline\n\
		    }\n\
		    else {\n\
		    macro = get_range(0,$text_length) "\\n"\n\
		    }\n\
		# back to the original window\n\
		focus_window(start_window)\n\
		# create temporary file\n\
		name = $file_path shell_command("date +%s","")\n\
		name = substring(name,0,length(name)-1)\n\
		t = read_file(name)\n\
		while ($read_status != 0) {\n\
		     name = $file_path shell_command("date +%s","")\n\
		     name = substring(name,0,length(name)-1)\n\
		     t = read_file(name)\n\
		     }\n\
		# here file that doesn't exist is found\n\
		write_file(macro, name)\n\
		load_macro_file(name)\n\
		# remove temporary file again\n\
		shell_command("rm -f " name,"")\n\
	}\n\
