nedit.smartIndentInit: 	C:	# C Macros and tuning parameters are shared with C++, and are declared\n\
	# in the common section.  Press Common / Shared Initialization above.\n\
	--End-of-Macro--\n\
	test = search("\\\\|\\\\>.*?\\\\<\\\\|", $cursor-1, "regex", "backward")\n\
	if ($cursor == $search_end) {\n\
	return -1\n\
	}\n\
	else {\n\
	return cFindSmartIndentDist($1)\n\
	}\n\
	--End-of-Macro--\n\
	if ($2 == "}" || $2 == "{" || $2 == "#")\n\
	    cBraceOrPound($1, $2)\n\
	--End-of-Macro--\n\
	C++:Default\n\
	Python:Default\n\
	Matlab:Default\n\
	C++:Default\n\
	Python:Default\n\
	Matlab:Default\n\
	C:Default\n\
	C++:Default\n\
	Python:Default\n\
	Matlab:Default\n\
	LaTeX:	lesstif_bug()\n\
	--End-of-Macro--\n\
	tcl_hook()\n\
	return 0\n\
	--End-of-Macro--\n\
	tcl_hook()\n\
	ltx_smart_indent($1, $2)\n\
	--End-of-Macro--\n\
	DTX:	set_wrap_margin(72)\n\
	--End-of-Macro--\n\
	return -1\n\
\n\
	--End-of-Macro--\n\
	if ($column >= $wrap_margin) {\n\
	  f = search("\\\\s", $cursor-1, "regex", "backward")\n\
	  if (f != -1) {\n\
	      right = search("$", $cursor, "regex", "nowrap")\n\
	      back = length(get_range($cursor,right))\n\
	      c = get_range(f+1, right)\n\
	      pos = search("^%*\\\\s*",$cursor,"regex","backward")\n\
	      set_cursor_pos(f+1)\n\
	      delete_to_end_of_line()\n\
	      insert_string("\\n" get_range(pos,$search_end) c)\n\
	      set_cursor_pos($cursor - back)\n\
	  }\n\
	}\n\
	--End-of-Macro--\n\
	Mail:	set_wrap_margin(68)\n\
	--End-of-Macro--\n\
	if ($column == 0) return 0\n\
\n\
	cpos = $cursor\n\
	pat_nr = "^\\\\s*(?:\\\\d\\\\.|\\\\l\\\\)|(?:o|\\\\-|\\\\*))\\\\s+"\n\
	pats = pat_nr "|^\\\\s*"\n\
	flag = 0\n\
	f = search(pats, $cursor, "regex", "backward")\n\
	if ($search_end != $cursor) {\n\
	    # get indent -> tabs: arrgh\n\
	    set_cursor_pos($search_end)\n\
	    indent = $column\n\
	    set_cursor_pos(cpos)\n\
	    return indent\n\
	}\n\
	else {\n\
	    f = search(pat_nr, $cursor, "regex", "backward")\n\
	    if (f != -1) {\n\
        	indent = measureIndent($search_end)\n\
        	while (f != -1 && indent >= $column) {\n\
        	    f = search(pat_nr, f - 1, "regex", "backward")\n\
        	    if (f != -1) indent = measureIndent($search_end)\n\
        	    flag = 1\n\
        	}\n\
        	if (indent == $column) indent = 0\n\
        	delete_to_start_of_line()\n\
        	if (flag == 1) delete_previous_character()\n\
        	return(indent)\n\
	    }\n\
	    else {\n\
        	return 0\n\
	    }\n\
\n\
	}\n\
	--End-of-Macro--\n\
	--End-of-Macro--\n\
	NEdit Macro:	--End-of-Macro--\n\
	return -1\n\
	--End-of-Macro--\n\
	--End-of-Macro--\n\
	SGML HTML:	if ($lesstif == "true") lesstif_bug()\n\
	--End-of-Macro--\n\
	return -1\n\
	--End-of-Macro--\n\
