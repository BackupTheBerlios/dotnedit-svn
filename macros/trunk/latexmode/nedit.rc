	Comments>Quote@LaTeX@SGML HTML:::: {\n\
		sign["NEdit Macro"] = "#"\n\
		sign["LaTeX"] = "%"\n\
		sign["Plain"] = "\\\\>"\n\
		sign["Setext"] = "\\\\.\\\\."\n\
		sign["X Resources"]="!"\n\
		sign["Tcl"] = "#"\n\
		\n\
		if ($language_mode in sign)\n\
		    {\n\
		    str = sign[ $language_mode] " &"\n\
		    if ($selection_start == -1)\n\
		    	{\n\
		    	replace_all("^.*$",str , "regex")\n\
			}\n\
			else\n\
			    {\n\
			    # cure if start of selection is not at start of line\n\
			    pos=$cursor\n\
			    beginning_of_selection()\n\
			    beginning_of_line("extend")\n\
			    set_cursor_pos(pos)\n\
			    replace_in_selection("^.*$", str, "regex")\n\
			    }\n\
		\n\
		    }\n\
		    else\n\
		        {\n\
			beep()\n\
			}\n\
	}\n\
	Comments>Unquote@LaTeX@SGML HTML:::: {\n\
		sign["NEdit Macro"] = "#"\n\
