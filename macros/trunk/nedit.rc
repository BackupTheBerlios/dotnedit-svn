	}\n\
	--End-of-Macro--\n\
	return -1\n\
	--End-of-Macro--\n\
	--End-of-Macro--\n\
	XML:	--End-of-Macro--\n\
	nspell_keyPress($1, "\\n")\n\
	return -1\n\
	--End-of-Macro--\n\
	nspell_keyPress($1, $2)\n\
	--End-of-Macro--\n\
