	NMacro>Insert Functions@NEdit Macro:Alt+1:::: {\n\
		found = search("^\\\\s*define\\\\s+\\\\w+\\\\s*\\\\{?$", 0, "regex")\n\
		routines = ""\n\
		while (found != -1) {\n\
		    next = $search_end\n\
		    tmp = get_range(found, next)\n\
		    tmp = replace_in_string(tmp, "^\\\\s*define\\\\s+", "", "regex")\n\
		    end = search_string(tmp, "\\\\s|$", 0, "regex")\n\
		    routine = substring(tmp, 0, end) "\\n"\n\
		    routines = routines routine\n\
		    \n\
		    # next\n\
		    found = search("^\\\\s*define\\\\s+\\\\w+\\\\s*\\\\{?$", next, "regex")\n\
		}\n\
		\n\
		insert_string(routines)\n\
	}\n\
