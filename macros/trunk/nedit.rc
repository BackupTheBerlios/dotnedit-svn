	ResText:	# Number of characters in a normal indent level.  May be a number, or the\n\
	# string "default", meaning, guess the value from the current tab settings.\n\
	$rstIndentDist = "default"\n\
\n\
	set_wrap_margin(72)\n\
\n\
	# for newline macro we require\n\
	jf_require("rst.nm")\n\
\n\
	jf_require("jf_fonts.nm")\n\
	jf_set_fontset("Semicondensed")\n\
	--End-of-Macro--\n\
	if (jf_require("keyPress.nm") == "") {\n\
	    nspell_keyPress($1, "\\n")\n\
	}\n\
\n\
	search("(?:^|[^:])::\\\\s*", $cursor, "regex", "backward")\n\
	if ($search_end == $cursor && $cursor != 0) {\n\
	    insert_string("\\n")\n\
	    return measureIndent($1) + defaultIndent($rstIndentDist)\n\
	}\n\
	return rst_smartindent_ret()\n\
	--End-of-Macro--\n\
	if (jf_require("keyPress.nm") == "") {\n\
	    nspell_keyPress($1, $2)\n\
	}\n\
	--End-of-Macro--\n\
