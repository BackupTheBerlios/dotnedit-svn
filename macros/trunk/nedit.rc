		replace_all("(?<!\\\\||\\\\[|\\\\l)[A-Z][a-z]+([A-Z][a-z]+)+", "[[&]]", "regex")\n\
		\n\
		# [some thing] -> [[some thing]]\n\
		replace_all("(?<!\\\\[)\\\\[[^\\\\|\\\\]\\\\[:]+\\\\]", "[&]", "regex")\n\
		\n\
		# [name | link] -> [[link | name]]\n\
		replace_all("(?<!\\\\[)\\\\[([^\\\\|\\\\]:]+)\\\\|([^\\\\|\\\\]:]+)\\\\]", "[[\\\\2|\\\\1]]", "regex")\n\
